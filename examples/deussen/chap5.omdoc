  <symbol name="left-congruence" role="object" scope="global">
    <metadata><dc:subject  xml:lang="de">Linkskongruenz $\rho $ auf $G$</dc:subject>
<dc:subject> left congruence</dc:subject></metadata>
  </symbol>


  <definition xml:id="left-congruence.def" for="#left-congruence" type="simple">
    <CMP xml:lang="de">
      <legacy format="TeX"> Sei $\rho $ eine "Aquivalenzrelation auf der
	Halbgruppe $F. \rho $ hei"st Linkskongruenz, wenn f"ur beliebige $g, f_1, f_2 \in
	F$ aus $f_1 \rho f_2$ folgt $(gf_1) \rho (gf_2)$ , Rechtskongruenz, wenn f"ur
	beliebige $g, f_1, f_2 \in F$ aus $f_1 \rho f_2$ folgt $(f_1g) \rho (f_2g)$ , und
	Kongruenz, wenn $\rho $ zugleich Links- und Rechtskongruenz ist. </legacy>
    </CMP>
      <OMOBJ xmlns="http://www.openmath.org/OpenMath">  
	<OMBIND>
	  <OMS cd="fns1" name="lambda"/>
	  <OMBVAR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/>
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="A"/>
		  <OMS cd="truthval" name="bool"/>
		</OMA>
	      </OMATP>
	      <OMV name="G"/>
	    </OMATTR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/>
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="G"/>
		  <OMA>
		    <OMS cd="simpletypes" name="funtype"/>
		    <OMV name="G"/>
		    <OMV name="G"/>
		  </OMA>
		</OMA>
	      </OMATP>
	      <OMV name="F"/>
	    </OMATTR>
	    <OMATTR>
	      <OMATP>
		<OMS cd="simpletypes" name="type"/>
		<OMA>
		  <OMS cd="simpletypes" name="funtype"/>
		  <OMV name="G"/>
		  <OMA>
		    <OMS cd="simpletypes" name="funtype"/>
		    <OMV name="G"/>
		    <OMS cd="truthval" name="bool"/>
		  </OMA>
		</OMA>
	      </OMATP>
	      <OMV name="RHO"/>
	    </OMATTR>
	  </OMBVAR>
	  <OMBIND>
	    <OMS cd="quant1" name="forall"/>
	    <OMBVAR>
	      <OMATTR>
		<OMATP>
		  <OMS cd="simpletypes" name="type"/>
		  <OMV name="G"/>
		</OMATP>
		<OMV name="G"/>
	      </OMATTR>
	      <OMATTR>
		<OMATP>
		  <OMS cd="simpletypes" name="type"/>
		  <OMV name="G"/>
		</OMATP>
		<OMV name="G1"/>
	      </OMATTR>
	      <OMATTR>
		<OMATP>
		  <OMS cd="simpletypes" name="type"/>
		  <OMV name="G"/>
		</OMATP>
		<OMV name="G2"/>
	      </OMATTR>
	    </OMBVAR>
	    <OMA>
	      <OMS cd="logic1" name="implies"/>
	      <OMA>
		<OMV name="RHO"/>
		<OMV name="G1"/>
		<OMV name="G2"/>
	      </OMA>
	      <OMA>
		<OMV name="RHO"/>
		<OMA>
		  <OMV name="F"/>
		  <OMV name="G"/>
		  <OMV name="G1"/>
		</OMA>
		<OMA>
		  <OMV name="F"/>
		  <OMV name="G"/>
		  <OMV name="G2"/>
		</OMA>
	      </OMA>
	    </OMA>
	  </OMBIND>
	</OMBIND>
      </OMOBJ>
  </definition>

